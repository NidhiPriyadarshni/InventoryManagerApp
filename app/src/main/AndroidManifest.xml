<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.inventorymanagerapp">

    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MakeSales"></activity>
        <activity android:name=".AddProduct" />
        <activity android:name=".SupplierList" />
        <activity android:name=".AdminList" />
        <activity android:name=".SalesList" />
        <activity
            android:name=".CategoryList"
            android:parentActivityName=".User" />
        <activity
            android:name=".ProductList"
            android:parentActivityName=".User" />
        <activity
            android:name=".Owner"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".User"
            android:parentActivityName=".MainActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>